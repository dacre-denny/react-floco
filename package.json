{
  "name": "react-floco",
  "version": "0.0.1",
  "description": "Conditional rendering helpers for react",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "dev": "tsc --watch",
    "tdd": "mocha -r ts-node/register -r tests/helpers --watch-extensions ts,tsx -b -w tests/*.spec.* -g dacre",
    "test": "mocha -r ts-node/register -r tests/helpers -b tests/*.spec.*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "eslintIgnore": [
    "dist",
    "examples",
    "tests"
  ],
  "keywords": [
    "react",
    "rendering"
  ],
  "author": "Dacre Denny",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/enzyme": "^3.10.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.4",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sinon": "^7.4.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {}
}
