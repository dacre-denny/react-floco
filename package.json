{
  "name": "react-floco",
  "version": "0.0.4",
  "description": "Conditional rendering helpers for react",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "lint": "eslint . --ext .ts,.tsx && remark .",
    "dev": "tsc --watch",
    "tdd": "mocha -r ts-node/register -r test/helpers --watch-extensions ts,tsx -b -w test/*.spec.*",
    "test": "mocha -r ts-node/register -r test/helpers -b test/*.spec.*",
    "coverage": "nyc mocha -r ts-node/register -r test/helpers -b test/*.spec.*",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dacre-denny/react-floco.git"
  },
  "keywords": [
    "react",
    "control flow",
    "declarative",
    "conditional",
    "switch",
    "case",
    "rendering"
  ],
  "author": "Dacre Denny",
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.0",
    "@types/enzyme": "^3.10.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/parser": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.4",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3",
    "remark-lint-list-item-indent": "^1.0.4",
    "sinon": "^7.4.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {}
}
